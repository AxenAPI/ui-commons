{
  "name": "@axenix/ui-kit",
  "version": "0.142.0",
  "author": "Axenix",
  "license": "MIT",
  "sideEffects": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "tsc": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:style": "stylelint \"**/*.{css,scss}\"",
    "lint:fix": "eslint src --ext ts,tsx --fix && stylelint \"**/*.{css,scss}\" --fix",
    "format": "prettier --write .",
    "qg": "run-p tsc lint",
    "preview": "vite preview",
    "sb:dev": "storybook dev -p 8573",
    "sb:build": "storybook build",
    "test": "jest --coverage",
    "analyze": "vite-bundle-visualizer"
  },
  "files": [
    "dist",
    "locales",
    "theme.schema.json"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./css": "./dist/style.css",
    "./locales": "./locales",
    "./rhf": {
      "import": "./dist/rhf.js",
      "require": "./dist/rhf.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "rhf": [
        "dist/rhf.d.ts"
      ]
    }
  },
